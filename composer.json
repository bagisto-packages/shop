{
    "name": "bagisto-packages/shop",
    "description": "Bagisto Shop plugin.",
    "license": "MIT",
    "require": {
        "algolia/algoliasearch-client-php": "^2.7",
        "astrotomic/laravel-translatable": "^11.9",
        "bagisto-packages/ui": "^1.0",
        "doctrine/dbal": "^2.12",
        "flynsarmy/db-blade-compiler": "^5.5",
        "guzzlehttp/guzzle": "^7.0",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5",
        "kalnoy/nestedset": "^5.0",
        "konekt/concord": "^1.8",
        "laravel/framework": "^8.14",
        "laravel/scout": "^8.4",
        "prettus/l5-repository": "^2.7",
        "tymon/jwt-auth": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Http/helpers.php"
        ],
        "psr-4": {
            "BagistoPackages\\Shop\\": "src/",
            "BagistoPackages\\Shop\\Seeds\\": "src/Resources/database/seeds"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
